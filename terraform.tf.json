{
  "terraform": [
    {
      "required_providers": [
        {
          "aws": {
            "source": "hashicorp/aws",
            "version": ">= 2.7.0"
          }
        }
      ]
    }
  ],
  "provider": [
    {
      "aws": [
        {
          "region": "us-west-2"
        }
      ]
    }
  ],
  "resource": [
    {
      "aws_s3_bucket": [
        {
          "terraform_backend_bucket": [
            {
              "bucket": "terraform-state-zw5fkq46rh8ckrft7ppidc566ejrcg7y1b43vcq8249qm"
            }
          ]
        }
      ]
    },
    {
      "aws_instance": [
        {
          "dbtest": [
            {
              "ami": "${data.aws_ami.windows_latest.id}",
              "instance_type": "t2.micro",
              "lifecycle": [
                {
                  "ignore_changes": [
                    "ami"
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "data": [
    {
      "aws_ami": [
        {
          "windows_latest": [
            {
              "most_recent": true,
              "owners": [
                "374168611083",
                "679593333241"
              ],
              "filter": [
                {
                  "name": "name",
                  "values": [
                    "*Windows Server*"
                  ]
                },
                {
                  "name": "virtualization-type",
                  "values": [
                    "hvm"
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
