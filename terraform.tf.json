{
  "terraform": [
    {
      "required_providers": [
        {
          "aws": {
            "source": "hashicorp/aws",
            "version": ">= 2.7.0"
          }
        }
      ]
    }
  ],
  "provider": [
    {
      "aws": [
        {
          "region": "us-west-2"
        }
      ]
    }
  ],
  "resource": [
    {
      "aws_s3_bucket": [
        {
          "terraform_backend_bucket": [
            {
              "bucket": "terraform-state-z4l8co21m3jpw0klp5rud752l1xg9o9emv78outq85jyw"
            }
          ]
        }
      ]
    },
    {
      "aws_instance": [
        {
          "test": [
            {
              "ami": "${data.aws_ami.amazon_latest.id}",
              "instance_type": "t2.medium",
              "lifecycle": [
                {
                  "ignore_changes": [
                    "ami"
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "data": [
    {
      "aws_ami": [
        {
          "amazon_latest": [
            {
              "most_recent": true,
              "owners": [
                "585441382316"
              ],
              "filter": [
                {
                  "name": "name",
                  "values": [
                    "*AmazonLinux*"
                  ]
                },
                {
                  "name": "virtualization-type",
                  "values": [
                    "hvm"
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
